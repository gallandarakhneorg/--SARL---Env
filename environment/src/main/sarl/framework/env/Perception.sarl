package framework.env


/** 
 * @author cd
 * 
 */
abstract class Perception {
	new(){}	
	
}	
class bodyPerception extends Perception {
	var xPosition : float
	var yPosition : float
	new( xPosition : float,  yPosition : float) {
		this.xPosition = xPosition
		this.yPosition = yPosition
	}
	def getxPosition : float {
		this.xPosition
	}
	def getyPosition : float {
		this.yPosition
	}
}
